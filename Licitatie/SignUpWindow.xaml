<Window x:Class="Licitatie.SignUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Licitatie"
        mc:Ignorable="d"
        Title="SignUpWindow" Height="300" Width="300">
    <Grid>
        <Label Content="Username" HorizontalAlignment="Left" Margin="38,45,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="38,114,0,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="usernameTxt" HorizontalAlignment="Left" Height="23" Margin="38,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="PasswordTxt" HorizontalAlignment="Left" Margin="38,145,0,0" VerticalAlignment="Top" Width="120" Height="24"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="38,188,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
